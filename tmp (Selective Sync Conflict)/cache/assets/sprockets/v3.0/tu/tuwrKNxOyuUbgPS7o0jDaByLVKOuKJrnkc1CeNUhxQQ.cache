I"&(function() {
  window.update_packs_select = function() {
    var level_count, pack_name, success_count;
    level_count = $('#levels li').length;
    success_count = $('#levels li .s-icon-star').length;
    pack_name = $('#packs').attr('data-pack-name');
    return $('#packs select option:selected').text(pack_name + " [" + success_count + "/" + level_count + "]");
  };

  $(function() {
    var level_button;
    if ($("#packs-show, #levels-show").length) {
      window.level_controller = new LevelController();
      level_button = $('#levels .is-selected');
      $('#levels').scrollTo(level_button, 1000, {
        easing: 'swing',
        offset: -10
      });
      $('#levels li, #levels li a').on('click', function() {
        $('#levels').find(".is-selected").removeClass('is-selected');
        $(this).closest('li').addClass('is-selected');
        window.change_level();
        window.push_this_state();
        return false;
      });
      $('#packs select').on('change', function() {
        var pack_name;
        pack_name = $('#packs select').val();
        return location.assign("/packs/" + pack_name);
      });
      $('#packs .packs-button a').on('click', function() {
        window.colorbox_random_level();
        return false;
      });
      $('#current-level-name h3').on('mouseenter', function() {
        return $('#current-level-description').fadeIn();
      });
      return $('#current-level-name h3').on('mouseleave', function() {
        return $('#current-level-description').stop().fadeOut();
      });
    }
  });

}).call(this);
