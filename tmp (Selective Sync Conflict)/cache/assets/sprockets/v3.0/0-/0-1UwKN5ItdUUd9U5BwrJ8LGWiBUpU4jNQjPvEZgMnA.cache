I"M(function() {
  $(function() {
    window.level_full_screen = false;
    $('#game').delegate('.fullscreen', 'click', function() {
      if (!window.level_full_screen) {
        window.level_full_screen = true;
      } else {
        window.level_full_screen = false;
      }
      return $('#game').fullScreen(window.level_full_screen);
    });
    return $(document).bind('fullscreenchange', function() {
      if (window.level_full_screen) {
        return $('#game .large-shad').hide();
      } else {
        return $('#game .large-shad').show();
      }
    });
  });

}).call(this);
