I"»(function() {
  $(function() {
    var bragging, hide_brag_anim, show_brag_anim;
    $('#scores').on('click', '#more-friend-score', function() {
      var friend_page, global_page;
      friend_page = parseInt($(this).attr('data-page')) + 1;
      global_page = parseInt($('#more-global-score').attr('data-page'));
      return window.reload_scores(friend_page, global_page);
    });
    $('#scores').on('click', '#more-global-score', function() {
      var friend_page, global_page;
      friend_page = parseInt($('#more-friend-score').attr('data-page'));
      global_page = parseInt($(this).attr('data-page')) + 1;
      return window.reload_scores(friend_page, global_page);
    });
    $('#scores').delegate('.score-item .brag', 'click', function() {
      if ($(this).attr('data-is-friend') === 'true') {
        window.facebook_send_to_feed_in_ladder($(this).attr('data-f_id'), $(this).attr('data-score-worse') === "true");
      } else {
        window.facebook_send_invitation_message($(this).attr('data-f_id'));
      }
      return false;
    });
    $('#game-won').on('click', '.final-score-item', function() {
      window.facebook_send_to_feed_in_ladder($(this).attr('data-f_id'), $(this).attr('data-score-worse') === "true", $('#final-score .score-pushes .num').html(), $('#final-score .score-moves .num').html());
      return false;
    });
    $('#game-won').on('click', '#share-on-fb', function() {
      return window.facebook_send_to_feed('', $('#final-score .score-pushes .num').html(), $('#final-score .score-moves .num').html());
    });
    bragging = false;
    $('#game-won').on('.final-score-item[data-score-worse="true"], .score-item[data-score-worse="true"]', 'mouseover', function() {
      $(this).parent().find('.brag').hide();
      $(this).parent().find('.score-name, .score-pushes').show();
      $(this).find('.score-name, .score-pushes').hide();
      $(this).find('.brag').show();
      return bragging = true;
    });
    $('#game-won').on('.final-score-item[data-score-worse="true"], .score-item[data-score-worse="true"]', 'mouseout', function() {
      $(this).find('.brag').hide();
      $(this).find('.score-name, .score-pushes').show();
      return bragging = false;
    });
    hide_brag_anim = function() {
      var worse_scores;
      if (!bragging) {
        worse_scores = $('.final-score-item[data-score-worse="true"]');
        worse_scores.find('.brag').hide();
        worse_scores.find('.score-name').show();
        worse_scores.find('.score-pushes').show();
      }
      return setTimeout(show_brag_anim, 3000);
    };
    show_brag_anim = function() {
      var worse_scores;
      if (!bragging) {
        worse_scores = $('.final-score-item[data-score-worse="true"]');
        worse_scores.find('.score-name').hide();
        worse_scores.find('.score-pushes').hide();
        worse_scores.find('.brag').show();
      }
      return setTimeout(hide_brag_anim, 500);
    };
    return setTimeout(show_brag_anim, 1000);
  });

}).call(this);
